  "peerDependencies": {
    "react": "^16.8.0"
  },
  "devDependencies": {
    "@types/react": "^16.9.25",
    "@types/resize-observer-browser": "^0.1.3",
    "husky": "^4.2.3",
    "prettier": "2.0.1",
    "prettier-plugin-organize-imports": "^1.0.2",
    "pretty-quick": "^2.0.1",
